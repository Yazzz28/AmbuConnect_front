{
  "name": "ambu-connect",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "prepare": "husky install",
    "serve": "./scripts/env-scripts/serve-with-env.sh $npm_config_env",
    "test": "jest --verbose",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "start-server": "npx http-server dist/frontend/browser -p 4200",
    "e2e:ui": "./scripts/env-scripts/e2e-ui-with-env.sh $npm_config_env",
    "e2e:ci": "./scripts/env-scripts/e2e-ci-with-env.sh $npm_config_env",
    "lint": "npx ng lint",
    "prettier": "npx prettier --write .",
    "doc": "node -e \"require('os').platform() === 'win32' ? require('child_process').execSync('start http://127.0.0.1:8080') : require('child_process').execSync('open http://127.0.0.1:8080')\" && compodoc -p tsconfig.doc.json -s"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.1.4",
    "@angular/common": "^19.1.4",
    "@angular/compiler": "^19.1.4",
    "@angular/core": "^19.1.4",
    "@angular/forms": "^19.1.4",
    "@angular/platform-browser": "^19.1.4",
    "@angular/platform-browser-dynamic": "^19.1.4",
    "@angular/router": "^19.1.4",
    "@fullcalendar/angular": "^6.1.16",
    "@fullcalendar/core": "^6.1.15",
    "@fullcalendar/daygrid": "^6.1.15",
    "@fullcalendar/interaction": "^6.1.15",
    "@fullcalendar/list": "^6.1.15",
    "@fullcalendar/timegrid": "^6.1.15",
    "@jaames/iro": "^5.5.2",
    "@primeng/themes": "^19.0.5",
    "primeicons": "^7.0.0",
    "primeng": "^19.0.6",
    "rxjs": "~7.8.0",
    "tailwindcss-primeui": "^0.4.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.1.5",
    "@angular/build": "^19.1.5",
    "@angular/cli": "^19.1.5",
    "@angular/compiler-cli": "^19.1.4",
    "@compodoc/compodoc": "^1.1.26",
    "@cypress/schematic": "^2.5.2",
    "@types/fullcalendar": "^3.8.0",
    "@types/jest": "^29.5.14",
    "angular-eslint": "18.4.1",
    "cypress": "latest",
    "eslint": "^9.15.0",
    "http-server": "^14.1.1",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-preset-angular": "^14.4.1",
    "start-server-and-test": "^2.0.9",
    "ts-node": "^10.9.2",
    "typescript": "~5.5.2",
    "typescript-eslint": "8.15.0",
    "wait-on": "^8.0.1"
  }
}
